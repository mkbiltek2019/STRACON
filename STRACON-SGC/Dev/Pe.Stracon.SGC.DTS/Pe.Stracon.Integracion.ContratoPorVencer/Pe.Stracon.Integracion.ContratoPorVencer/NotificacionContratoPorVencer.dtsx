          SQLTask:SqlStatementSource="DECLARE @TABLA_HTML&#x9;&#x9;&#x9;NVARCHAR(MAX)&#xA;DECLARE @EMISOR&#x9;&#x9;&#x9;&#x9;NVARCHAR(50)&#xA;DECLARE @DESTINATARIO&#x9;&#x9;NVARCHAR(MAX)&#xA;DECLARE @DESTINATARIO_COPIA NVARCHAR(MAX)&#xA;DECLARE @ASUNTO&#x9;&#x9;&#x9;&#x9;NVARCHAR(MAX)&#xA;DECLARE @CONTENIDO&#x9;&#x9;    NVARCHAR(MAX)&#xA;DECLARE @FORMATO_CONTENIDO  NVARCHAR(10)&#xA;DECLARE @IMPORTANCIA&#x9;    NVARCHAR(10)&#xA;&#xA;SET LANGUAGE spanish;&#xA;SET DATEFORMAT  dmy;&#xA;&#xA;SET&#x9;@FORMATO_CONTENIDO = 'html'&#xA;&#xA;SET&#x9;@IMPORTANCIA = 'High'&#xA;&#xA;SET&#x9;&#x9;@TABLA_HTML =&#xA;&#x9;&#x9;N'&lt;br&gt;' +&#xA;&#x9;&#x9;N'&lt;br&gt;' +&#xA;       &#x9;N'&lt;table border=&quot;1px solid #CCCCC&quot; width=&quot;100%&quot; cellspacing=&quot;0&quot; style=&quot;text-align: center; vertical-align: middle&quot;&gt;' +&#xA;       &#x9;N'&lt;tr&gt; &lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Unidad Operativa&lt;/th&gt;' +&#xA;        N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Número Contrato&lt;/th&gt;' +&#xA;&#x9;&#x9;N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Número Adenda&lt;/th&gt;' +&#xA;&#x9;&#x9;N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;RUC Proveedor&lt;/th&gt;' +&#xA;&#x9;&#x9;N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Proveedor&lt;/th&gt;' +&#xA;&#x9;&#x9;N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Fecha Vencimiento&lt;/th&gt;' +&#xA;&#x9;&#x9;N'&lt;th style=&quot;text-align: center; vertical-align: middle; background-color: #E3E3E3; border-color: black; border: 1px solid&quot;&gt;Dias Restantes&lt;/th&gt;&lt;/tr&gt;' +&#xA;        CAST ( ( SELECT td = NOMBRE, '',&#xA;                        td = NUMERO_CONTRATO, '',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;td = NUMERO_ADENDA_CONCATENADO, '',&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;td = NUMERO_DOCUMENTO, '',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;td = PROVEEDOR, '',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;td = CONVERT(VARCHAR(50),FECHA_VENCIMIENTO,103), '',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;td = DIAS_VENCIMIENTO, ''&#xA;                  FROM TMP.CONTRATO_POR_VENCER&#xA;                  FOR XML PATH('tr'), TYPE &#xA;        ) AS NVARCHAR(MAX) ) +      &#xA;        N'&lt;/table&gt;' +        &#xA;        N'&lt;br&gt;'&#xA;&#xA;SET @TABLA_HTML = REPLACE(@TABLA_HTML,'&lt;td&gt;','&lt;td style=&quot;text-align: center; vertical-align: middle; border-color: black; border: 1px solid&quot;&gt;')&#xA;--PRINT @TABLA_HTML&#xA;&#xA;SELECT&#x9;DISTINCT&#xA;&#x9;&#x9;@EMISOR&#x9;&#x9;&#x9;&#x9;= EMISOR,&#xA;&#x9;&#x9;@DESTINATARIO&#x9;&#x9;= DESTINATARIO,&#xA;&#x9;&#x9;@DESTINATARIO_COPIA = DESTINATARIO_COPIA,&#xA;&#x9;&#x9;@ASUNTO&#x9;&#x9;&#x9;&#x9;= ASUNTO,&#xA;&#x9;&#x9;@CONTENIDO&#x9;&#x9;&#x9;= REPLACE(CONTENIDO, '@lista_contratos_por_vencer', @TABLA_HTML)&#xA;FROM&#x9;TMP.CONTRATO_POR_VENCER&#xA;&#xA;&#xA;EXEC&#x9;[MSDB].[DBO].[SP_SEND_DBMAIL]&#x9;@PROFILE_NAME&#x9; = @EMISOR, &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;@RECIPIENTS&#x9;&#x9; = @DESTINATARIO,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;@BODY&#x9;&#x9;&#x9; = @CONTENIDO,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;@COPY_RECIPIENTS = @DESTINATARIO_COPIA,&#xA;                                        @BODY_FORMAT     = @FORMATO_CONTENIDO,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;@SUBJECT         = @ASUNTO,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;@IMPORTANCE      = @IMPORTANCIA" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
