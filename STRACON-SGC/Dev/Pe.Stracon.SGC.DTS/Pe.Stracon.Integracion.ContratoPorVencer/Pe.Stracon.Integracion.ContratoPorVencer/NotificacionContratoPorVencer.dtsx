                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @CODIGO_SISTEMA UNIQUEIDENTIFIER
DECLARE @CONTENIDO NVARCHAR(MAX)
DECLARE @ASUNTO NVARCHAR(MAX)
DECLARE @EMISOR NVARCHAR(50)
DECLARE @DESTINATARIO NVARCHAR(MAX)
DECLARE @DESTINATARIO_COPIA NVARCHAR(MAX)
DECLARE @CANTIDAD_DIAS INT

SELECT	@EMISOR = VALOR
FROM	GRL.FN_LISTAR_VALORES_PARAMETRO('STR',0,'SGC','00034',3)
WHERE	CODIGO = '01'

SELECT	@CANTIDAD_DIAS = CONVERT(INT,VALOR)
FROM	GRL.FN_LISTAR_VALORES_PARAMETRO('STR',0,'SGC','00031',3)
WHERE	CODIGO = '02'

SELECT	@CODIGO_SISTEMA = CODIGO_SISTEMA
FROM	GRL.SISTEMA
WHERE	CODIGO_IDENTIFICADOR = 'SGC'

SELECT	@CONTENIDO = CONTENIDO,
        @ASUNTO = ASUNTO,
		@DESTINATARIO = DESTINATARIO,
		@DESTINATARIO_COPIA = DESTINATARIO_COPIA
FROM	GRL.PLANTILLA_NOTIFICACION
WHERE	CODIGO_SISTEMA = @CODIGO_SISTEMA
AND		CODIGO_TIPO_NOTIFICACION = '01'

SELECT	CODIGO_UNIDAD_OPERATIVA,
		NOMBRE,
		@CONTENIDO CONTENIDO,
		@ASUNTO ASUNTO,
        @EMISOR EMISOR,
		@DESTINATARIO DESTINATARIO,
		@DESTINATARIO_COPIA DESTINATARIO_COPIA,
                                      @CANTIDAD_DIAS CANTIDAD_DIAS
FROM	GRL.UNIDAD_OPERATIVA
WHERE	ESTADO_REGISTRO = '1'</property>
