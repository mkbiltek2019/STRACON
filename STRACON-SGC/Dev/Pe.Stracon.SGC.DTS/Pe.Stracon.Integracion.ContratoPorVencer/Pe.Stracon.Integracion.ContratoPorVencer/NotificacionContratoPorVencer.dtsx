                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SET LANGUAGE spanish;
SET DATEFORMAT  dmy;
SELECT	
	CON.CODIGO_UNIDAD_OPERATIVA,
	CON.NUMERO_CONTRATO,
	ISNULL(con.NUMERO_ADENDA_CONCATENADO,'-')AS NUMERO_ADENDA_CONCATENADO,
	PRO.NUMERO_DOCUMENTO,
	PRO.NOMBRE										AS PROVEEDOR,
	CON.FECHA_FIN_VIGENCIA	AS FECHA_VENCIMIENTO,
    DATEDIFF(DD,GETDATE(),FECHA_FIN_VIGENCIA)		AS CANTIDAD_DIAS
FROM	CTR.CONTRATO CON
INNER JOIN CTR.PROVEEDOR PRO ON PRO.CODIGO_PROVEEDOR = CON.CODIGO_PROVEEDOR AND PRO.ESTADO_REGISTRO = '1'
WHERE	CON.CODIGO_ESTADO = 'V'
