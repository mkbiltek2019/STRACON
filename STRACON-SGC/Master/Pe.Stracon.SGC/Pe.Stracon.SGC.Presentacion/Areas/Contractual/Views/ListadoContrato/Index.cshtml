@using Pe.Stracon.SGC.Presentacion.Recursos.Base
@using Pe.Stracon.SGC.Presentacion.Recursos.Contractual
@using Pe.Stracon.SGC.Presentacion.Recursos.Mensajes
@using Pe.Stracon.SGC.Presentacion.Core.Helpers
@using Pe.Stracon.SGC.Infraestructura.Core.Context
@using Pe.Stracon.SGC.Aplicacion.Core.Message

@model Pe.Stracon.SGC.Presentacion.Core.ViewModel.Contractual.ListadoContrato.ListadoContratoBusqueda

@{
    var disabled = (Model.ControlPermisos.ControlTotal || Model.ControlPermisos.Escritura) ? false : true;
}

<div class="GmdBody-header">
    <h2>@ListadoContratoResource.EtiquetaTituloPrincipal</h2>
</div>
<div class="GmdBody-body">

    <div class="panel-estudio-asignado panel-group" id="divVisualizarFiltroBusqueda" role="tablist" aria-multiselectable="true">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                    <a class="collapsed" data-toggle="collapse" data-parent="#divVisualizarFiltroBusqueda" href="#divAcordionFiltroBusqueda">
                        <div class="title-form">Reducir / Expandir Filtros</div>
                        @*<div class="pull-right control-arrow">
                            <i class="fa fa-angle-down"></i>
                        </div>*@
                    </a>
                </h4>
            </div>
            <div id="divAcordionFiltroBusqueda" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                <form id="frmContratoBusqueda">
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcTipoServicio">@ListadoContratoResource.EtiquetaUnidadOperativa</label>
                                @Html.DropDownList("slcUnidadOperativa", @Model.ListadoUnidadOperativa, new { @class = "form-control", @id = "slcUnidadOperativa" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcTipoServicio">@ListadoContratoResource.EtiquetaTipoServicio</label>
                                @Html.DropDownList("slcTipoServicio", @Model.TipoServicio, new { @class = "form-control", @id = "slcTipoServicio" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcTipoRequerimiento">@ListadoContratoResource.EtiquetaTipoRequerimiento</label>
                                @Html.DropDownList("slcTipoRequerimiento", new List<SelectListItem>(), new { @class = "form-control", @id = "slcTipoRequerimiento" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcTipoDocumento">@ListadoContratoResource.EtiquetaTipoDocumento</label>
                                @Html.DropDownList("slcTipoDocumento", @Model.TipoDocumento, new { @class = "form-control", @id = "slcTipoDocumento" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcEstadoContrato">@ListadoContratoResource.EtiquetaEstadoContrato</label>
                                @Html.DropDownList("slcEstadoContrato", @Model.EstadoContrato, new { @class = "form-control", @id = "slcEstadoContrato" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="slcEstadoContrato">@ListadoContratoResource.EtiquetaEstadoEdicion</label>
                                @Html.DropDownList("slcEstadoEdicion", @Model.EstadoEdicion, new { @class = "form-control", @id = "slcEstadoEdicion" })
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="txtNumeroContrato">@ListadoContratoResource.EtiquetaNumeroContrato</label>
                                @Html.TextBox("txtNumeroContrato", null, new { @class = "form-control", @id = "txtNumeroContrato" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="txtNumeroAdenda">@ListadoContratoResource.EtiquetaNumeroAdenda</label>
                                @Html.TextBox("txtNumeroAdenda", null, new { @class = "form-control", @id = "txtNumeroAdenda" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="txtNumeroDocumentoProveedor">@ListadoContratoResource.EtiquetaRucProveedor</label>
                                @Html.TextBox("txtNumeroDocumentoProveedor", null, new { @class = "form-control", @id = "txtNumeroDocumentoProveedor" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="txtNombreProveedor">@ListadoContratoResource.EtiquetaNombreProveedor</label>
                                @Html.TextBox("txtNombreProveedor", null, new { @class = "form-control", @id = "txtNombreProveedor" })
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label for="txtDescripcion">@ListadoContratoResource.EtiquetaDescripcion</label>
                                @Html.TextBox("txtDescripcion", null, new { @class = "form-control", @id = "txtDescripcion" })
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="textContenidoContrato">@ListadoContratoResource.EtiquetaContenidoContrato</label>
                                @Html.TextBox("textContenidoContrato", null, new { @class = "form-control", @id = "textContenidoContrato" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label>@ListadoContratoResource.EtiquetaBandeja</label>
                                @Html.TextBox("txtEnBandejaDe", null, new { @class = "form-control", @id = "txtEnBandejaDe" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label>@ListadoContratoResource.EtiquetaEstadio</label>
                                @Html.DropDownList("slcEstadio", @Model.ListaEstadio, new { @class = "form-control", @id = "slcEstadio" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label>@ListadoContratoResource.EtiquetaTipoMoneda</label>
                                @Html.DropDownList("slcMoneda", @Model.Moneda, new { @class = "form-control", @id = "slcMoneda" })
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label>@ReporteListadoContratoResource.EtiquetaMontoAcumuladoInicio</label>
                                <div class="input-group">
                                    @Html.TextBox("txtMontoAcumuladoInicio", @Model.MontoAcumuladoInicio, new { @class = "form-control", @id = "txtMontoAcumuladoInicio" })
                                    <div class="input-group-addon"><span class="fa fa-usd"></span></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label>@ReporteListadoContratoResource.EtiquetaMontoAcumuladoFin</label>
                                <div class="input-group">
                                    @Html.TextBox("txtMontoAcumuladoFin", @Model.MontoAcumuladoFin, new { @class = "form-control", @id = "txtMontoAcumuladoFin" })
                                    <div class="input-group-addon"><span class="fa fa-usd"></span></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <label for="textContenidoContrato">Usuario Creación</label>
                                @Html.TextBox("textUsuarioCreacionContrato", null, new { @class = "form-control", @id = "textUsuarioCreacionContrato" })
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>@ListadoContratoResource.EtiquetaFinalizarAprobacion</label>
                                <div class="row">
                                    <div class="col-sm-5">
                                        <input type="radio" value="" name="rbIndicadorFinalizarAprobacion" checked="checked">
                                        <label>@ListadoContratoResource.EtiquetaTodos</label>
                                    </div>
                                    <div class="col-sm-3">
                                        <input type="radio" value='@Model.Aprobado' name="rbIndicadorFinalizarAprobacion">
                                        <label>@ListadoContratoResource.EtiquetaSi</label>
                                    </div>
                                    <div class="col-sm-3">
                                        <input type="radio" value='@Model.NoAprobado' name="rbIndicadorFinalizarAprobacion">
                                        <label>@ListadoContratoResource.EtiquetaNo</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <button class="btn btn-info" id="btnLimpiar" type="reset">@GenericoResource.BotonLimpiar</button>
                            <button class="btn btn-default" id="btnBuscar">@GenericoResource.BotonMostrar</button>
                        </div>
                    </div>
                </div>
            
            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-sm-6 text-left">
            <div class="form-group">
                <h3>@GenericoResource.EtiquetaResultadosBusqueda</h3>
            </div>
        </div>
        <div class="col-sm-6 text-right">
            <div class="form-group">
                <button id="btnCopiarContrato" class="btn btn-info">Copiar Contrato</button>

                @if (!disabled)
                {
                    <button id="btnAgregar" class="btn btn-primary">@GenericoResource.BotonAñadir</button>
                }
            </div>
        </div>
    </div>
    <div id="divGrdResult">
    </div>
</div>

<style type="text/css">
    p {
        margin: 0 0 15px;
        font-size: 10.5px;
        color: #565656;
    }
</style>

